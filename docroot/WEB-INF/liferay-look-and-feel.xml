<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.0.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_0_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.1.0+</version>
	</compatibility>
	<theme id="gnenc" name="GNENC Theme">
		<root-path>/</root-path>
	 	<images-path>${root-path}/images</images-path>
    	<color-scheme id="01" name="Yellow">
       	 	<css-class>yellow</css-class>
        	<color-scheme-images-path>${images-path}/color_schemes/${css-class}</color-scheme-images-path>
    	</color-scheme>
    	<color-scheme id="02" name="Blue">
       		<css-class>blue</css-class>
    	</color-scheme>
    </theme>
</look-and-feel>